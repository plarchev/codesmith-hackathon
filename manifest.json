{
  "manifest_version": 3,
  "permissions": [
    "geolocation"
  ],
  "name": "Weather App Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "weather-extension.html",
    "default_icon": "icons8-weather-16.png"
  },
  
  "icons": {
    "16": "icons8-weather-16.png",
    "32": "icons8-weather-32.png",
    "48": "icons8-weather-48.png",
    "128": "icons8-weather-128.png"
  }
}

